<ul>
    <!-- Ponemos en el for todas las variables para que nos saque todos los campos de la base de datos con ese id -->
      {% for turismo in turismos %}
        <h2>{{ turismo.localidad }}</h2>
        <p>{{turismo.poblacion}}, {{turismo.provincia}}</p>
        <p>{{turismo.fechaReserva|date('Y-m-d')}}</p>
        {% endfor %}
</ul>
